                  <a {% if content.field_link_1[0]['#url_title'] |render|striptags|trim|length > 0 %} href="{{ content.field_link_1[0]['#url_title'] |render|striptags|trim }}"{% endif %}
                    class=""
                    target="_blank">
                    <img class="img-fluid w-100" src="{{content.field_promo_image_1 |render|striptags|trim }}" alt="" />
                    {% if content.field_texto_1 |render|striptags|trim != '' %}
